{
    "extends": "airbnb",
    "env": {
      "browser":true,
      "node": true
    },
    "rules":{
      "import/no-unresolved": 0,
      "class-methods-use-this":0,
      "no-continue":0,
      "prefer-destructuring":0,
      "no-plusplus": 0,
      "react/jsx-filename-extension": 0,
      "react/destructuring-assignment":0,
      "react/prop-types":0,
      "no-lonely-if":0,
      "react/no-array-index-key":0,
      "no-unused-vars":0,
      "no-alert":0
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
}